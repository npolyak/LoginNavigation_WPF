<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="300"
			 xmlns:styles="clr-namespace:Material.Styles;assembly=Material.Styles"
			 xmlns:controls="clr-namespace:Material.Styles.Controls;assembly=Material.Styles"
             x:Class="Mac_project.Views.RunScreen.MainGHSECSGEM._2DConfiguration">

	<controls:Card>
		<StackPanel Orientation="Vertical" Margin="10 5">
			<TextBlock Classes="Subtitle1" Text="Parameters Settings" FontWeight="Medium"/>
			<StackPanel Orientation="Vertical" Margin="0 10">
				<CheckBox IsChecked="False">Strips Map Download</CheckBox>
				<CheckBox IsChecked="False">Strips Map Upload</CheckBox>
				<CheckBox IsChecked="False">Strip ID Map Manual Key In</CheckBox>
				<CheckBox IsChecked="False">Lot Management</CheckBox>
				<CheckBox IsChecked="False">Check Strip ToGo</CheckBox>
				<CheckBox IsChecked="True">Strip ID Checked</CheckBox>
				<CheckBox IsChecked="False">Wafer Map Download</CheckBox>
				<CheckBox IsChecked="True">Auto Download Recipe</CheckBox>
				<CheckBox IsChecked="True">Delete Recipe</CheckBox>
			</StackPanel>
		</StackPanel>
	</controls:Card>
</UserControl>
