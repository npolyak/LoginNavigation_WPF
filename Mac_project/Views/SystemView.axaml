<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 xmlns:vm="using:Mac_project.ViewModels"
			 xmlns:systemUsers="clr-namespace:Mac_project.Views.System.Users;assembly=Mac_project"
			 xmlns:systemFiles="clr-namespace:Mac_project.Views.System.Files;assembly=Mac_project"
			 x:Class="Mac_project.Views.SystemView">

	<Grid RowDefinitions="Auto, *" Margin="5">
		<TextBlock Grid.Row="0" Classes="Headline6" Text="System"/>

		<Border Grid.Row="1" Margin="5">
			<StackPanel>
				<TabControl>
					<TabControl.Items>
						<TabItem Header="File" Foreground="White" FontSize="16">
							<TextBlock Classes="Centered" Text="Page 1"/>
						</TabItem>
						<TabItem Header="Users" Foreground="White" FontSize="16">
							<systemUsers:mainUsers/>
						</TabItem>
						<TabItem Header="Parameters" Foreground="White" FontSize="16">
							<TextBlock Classes="Centered" Text="Page 3"/>
						</TabItem>
					</TabControl.Items>
				</TabControl>
			</StackPanel>
		</Border>
	</Grid>
</UserControl>
