<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:viewModels="clr-namespace:Mac_project.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Mac_project.Views.MainDashboard"
			 x:DataType="viewModels:DashboardViewModel">
	
	<Grid RowDefinitions="Auto, *" Margin="10">
		<TextBlock Grid.Row="0" Margin="0 10" FontSize="20" FontWeight="Bold">
			Dashboard
		</TextBlock>
		<Border Grid.Row="1" Background="#2d323c" CornerRadius="10">
			<Image Source="/Assets/home.jpg" Height="320"/>
		</Border>
	</Grid>
</UserControl>
